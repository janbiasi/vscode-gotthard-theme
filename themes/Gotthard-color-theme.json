{
  "name": "Gotthard",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "focusBorder": "#E9B44C",

    // scrollbars
    "scrollbarSlider.background": "#191919",
    "scrollbarSlider.activeBackground": "#292929",
    "scrollbarSlider.hoverBackground": "#292929",

    // generic lists
    "list.errorForeground": "#AB6369",
    "list.invalidItemForeground": "#CAAB6D",
    "list.warningForeground": "#CAAB6D",
    "list.hoverBackground": "#191919",
    "list.activeSelectionBackground": "#191919",

    // buttons
    "button.background": "#191919",
    "button.hoverBackground": "#292929",

    // main editor view
    "editor.background": "#000000",
    "editor.foreground": "#F5F5F5",
    "editor.wordHighlightBackground": "#292929",
    "editor.selectionBackground": "#292929",
    "editor.selectionHighlightBackground": "#292929",
    "editor.selectionHighlightBorder": "#707070",
    "editor.findMatchBackground": "#292929",
    "editor.findMatchBorder": "#707070",

    // editor groups
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.border": "#191919",
    "editorGroupHeader.tabsBorder": "#191919",
    "editorGroup.border": "#191919",

    // editor hints
    "editorError.foreground": "#ab6369",
    "editorWarning.foreground": "#caab6d",
    "editorInfo.foreground": "#6f98b3",

    // minimap view
    "minimap.background": "#000000",
    "minimapSlider.background": "#141414",

    // debugger breakpoints
    "debugIcon.breakpointForeground": "#AB6369",
    "debugIcon.breakpointDisabledForeground": "#1D201F",

    // main tabs
    "tab.border": "#1F1F1F",
    "tab.activeBackground": "#000000",
    "tab.activeForeground": "#F5F5F5",
    "tab.inactiveForeground": "#999999",
    "tab.inactiveBackground": "#191919",
    "tab.unfocusedInactiveForeground": "#ff0000",
    "tab.activeModifiedBorder": "#E9B44C",
    "tab.inactiveModifiedBorder": "#E9B44C",
    "tab.unfocusedActiveModifiedBorder": "#E9B44C",
    "tab.unfocusedInactiveModifiedBorder": "#E9B44C",

    // sidebar
    "sideBar.background": "#000000",
    "sideBar.border": "#191919",
    "sideBarTitle.foreground": "#F5F5F5",
    "sideBarSectionHeader.background": "#191919",

    // title bar (where the settings in windows are + title)
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#F5F5F5",
    "titleBar.border": "#191919",

    // action panel (eg. terminal)
    "panel.background": "#000000",

    // menu (dropdowns at the top)
    "menu.background": "#000000",
    "menu.border": "#191919",
    "menu.separatorBackground": "#707070",

    // status bar (the bottom bar)
    "statusBar.background": "#000000",
    "statusBar.foreground": "#f5f5f5",
    "statusBar.border": "#191919",
    "statusBar.debuggingBackground": "#AB6369",
    "statusBar.debuggingForeground": "#f5f5f5",

    // activity bar (where scm, search, etc. lives in)
    "activityBar.background": "#000000",
    "activityBar.foreground": "#f5f5f5",
    "activityBar.inactiveForeground": "#707070",
    "activityBar.activeBorder": "#f5f5f5",
    "activityBar.border": "#1F1F1F",
    "activityBarBadge.foreground": "#f5f5f5",
    "activityBarBadge.background": "#836781",

    // source management control
    "gitDecoration.addedResourceForeground": "#81B19B",
    "gitDecoration.deletedResourceForeground": "#AB6369",
    "gitDecoration.conflictingResourceForeground": "#AB6369",
    "gitDecoration.modifiedResourceForeground": "#CAAB6D",
    "gitDecoration.untrackedResourceForeground": "#81B19B",
    "gitDecoration.renamedResourceForeground": "#CAAB6D",
    "gitDecoration.ignoredResourceForeground": "#777",

    // icons
    "problemsErrorIcon.foreground": "#AB6369",
    "problemsInfoIcon.foreground": "#6F98B3",
    "problemsWarningIcon.foreground": "#CAAB6D",

    // notifications
    "notifications.background": "#000000",
    "notifications.border": "#191919",
    "notificationCenter.border": "#191919",
    "notificationLink.foreground": "#CAAB6D"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#5C6666"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#D2D2D2"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#d2d2d2"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#967994"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#836781"
      }
    },
    {
      "name": "Punctuation - String start and end",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#B89784"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#6F98B3"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#6F98B3"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },

    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },

    // JSON
    {
      "name": "JSON - Keys",
      "scope": [
        "source.json meta.structure.dictionary support.type.property-name",
        "source.json punctuation.support.type.property-name.begin",
        "source.json punctuation.support.type.property-name.end"
      ],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "JSON - Values",
      "scope": [
        "source.json string",
        "source.json meta.structure.dictionary punctuation",
        "source.json meta.structure.dictionary punctuation.definition.string.begin",
        "source.json meta.structure.dictionary punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },

    // Markdown
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },

    // Custom: Specific overrides
    {
      "name": "Terminator Semicolon",
      "scope": ["punctuation.terminator"],
      "settings": {
        "foreground": "#D2D2D2"
      }
    },

    // Custom: HTML
    {
      "name": "HTML - Tags",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#836781"
      }
    },
    {
      "name": "HTML - Tag begin and end",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },

    // Custom: TypeScript
    {
      "name": "TypeScript - Named Import",
      "scope": ["meta.block.tsx variable.other.readwrite.alias.tsx"],
      "settings": {
        "foreground": "#B89784"
      }
    },
    {
      "name": "TypeScript - Type Declaration, Enum and Annotations",
      "scope": [
        "meta.type.annotation.tsx",
        "meta.interface.tsx",
        "meta.parameter.object-binding-pattern.tsx variable"
      ],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "TypeScript - Null and Undefined",
      "scope": [
        "constant.language.undefined.tsx",
        "constant.language.null.tsx"
      ],
      "settings": {
        "foreground": "#ab6369"
      }
    },
    {
      "name": "TypeScript - This",
      "scope": ["variable.language.this.ts"],
      "settings": {
        "foreground": "#81b19b",
        "fontStyle": ""
      }
    },
    {
      "name": "TypeScript - Enum property",
      "scope": ["variable.other.constant.property.tsx"],
      "settings": {
        "foreground": "#6F98B3",
        "fontStyle": "bold"
      }
    },

    // Custom: React
    {
      "name": "React - Block",
      "scope": ["punctuation.definition.block.tsx"],
      "settings": {
        "foreground": "#D2D2D2"
      }
    },
    {
      "name": "React - JSX Tags",
      "scope": [
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "React - JSX HTML Attributes",
      "scope": [
        "meta.tag.attributes.tsx string",
        "meta.tag.attributes.tsx punctuation.definition.string.begin.tsx",
        "meta.tag.attributes.tsx punctuation.definition.string.end.tsx"
      ],
      "settings": {
        "foreground": "#6F98B3"
      }
    },
    {
      "name": "React- Children",
      "scope": ["source.tsx meta.jsx.children.tsx"],
      "settings": {
        "foreground": "#FAFAFA"
      }
    },

    // Custom: CSS
    {
      "name": "CSS - Brackets",
      "scope": [
        "punctuation.section.property-list.begin.bracket.curly.scss",
        "punctuation.section.property-list.end.bracket.curly.scss"
      ],
      "settings": {
        "foreground": "#D2D2D2"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "source.css.scss entity.other.attribute-name.class",
        "source.css.scss entity.other.attribute-name.class.css punctuation"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.css.scss entity.other.attribute-name.id.css",
        "source.css.scss entity.other.attribute-name.id.css punctuation"
      ],
      "settings": {
        "foreground": "#AB6369"
      }
    },
    {
      "name": "CSS - Property name",
      "scope": ["source.css support.type.property-name"],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "CSS - Property value",
      "scope": [
        "meta.property-value.scss",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#D2D2D2"
      }
    },
    {
      "name": "CSS - Strings",
      "scope": [
        "source.css.scss string",
        "source.css.scss string.quoted.double.scss"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "CSS - Colors",
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "constant.other.color.rgb-value.hex.css punctuation"
      ],
      "settings": {
        "foreground": "#B89784"
      }
    },

    // Custom: SCSS
    {
      "name": "SCSS - Expressions",
      "scope": [
        "keyword.control.at-rule.use.scss",
        "meta.at-rule.use.scss",
        "punctuation.definition.keyword.scss",
        "source.css.scss keyword.control"
      ],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "SCSS - Variable",
      "scope": ["variable.scss"],
      "settings": {
        "foreground": "#6F98B3"
      }
    },

    // Custom: Java
    {
      "name": "Java - Default code",
      "scope": ["source.java"],
      "settings": {
        "foreground": "#D2D2D2"
      }
    },
    {
      "name": "Java - Storage Type",
      "scope": ["storage.type.java"],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "Java - Object and Properties",
      "scope": [
        "variable.other.object.java",
        "variable.other.object.property.java"
      ],
      "settings": {
        "foreground": "#6F98B3"
      }
    },
    {
      "name": "Java - Modifiers",
      "scope": ["storage.modifier.java"],
      "settings": {
        "foreground": "#836781"
      }
    },
    {
      "name": "Java - Class",
      "scope": ["meta.class.identifier.java storage.modifier.java"],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "Java - Class Inheritance",
      "scope": ["entity.other.inherited-class.java"],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "Java - Interface extension",
      "scope": [
        "storage.modifier.extends.java",
        "storage.modifier.implements.java"
      ],
      "settings": {
        "foreground": "#836781"
      }
    },
    {
      "name": "Java - Annotation",
      "scope": [
        "punctuation.definition.annotation.java",
        "meta.declaration.annotation.java",
        "storage.type.annotation.java",
        "meta.declaration.annotation.java"
      ],
      "settings": {
        "foreground": "#81B19B"
      }
    },

    // Custom: Doc Comments (JavaDoc, TypeDoc)
    {
      "name": "Comment Annotations",
      "scope": [
        "comment punctuation",
