{
  "name": "Gotthard",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "foreground": "#F5F5F5",
    "focusBorder": "#836781",
    "icon.foreground": "#F5F5F5",

    // windows
    "window.activeBorder": "#000000",
    "window.inactiveBorder": "#292929",

    // links
    "textLink.foreground": "#836781",
    "textLink.activeForeground": "#b19db4",

    // checkboxes
    "checkbox.background": "#836781",
    "settings.checkboxBackground": "#292929",
    "settings.checkboxForeground": "#836781",

    // dropdowns
    "dropdown.background": "#000000",
    "dropdown.border": "#292929",
    "settings.dropdownBorder": "#292929",

    // inputs
    "input.background": "#000000",
    "input.border": "#292929",

    // progress bars
    "progressBar.background": "#836781",

    // scrollbars
    "scrollbarSlider.background": "#191919",
    "scrollbarSlider.activeBackground": "#292929",
    "scrollbarSlider.hoverBackground": "#292929",

    // generic lists
    "list.errorForeground": "#AB6369",
    "list.invalidItemForeground": "#CAAB6D",
    "list.warningForeground": "#CAAB6D",
    "list.hoverBackground": "#191919",
    "list.activeSelectionBackground": "#191919",
    "list.highlightForeground": "#836781",

    // buttons
    "button.background": "#836781",
    "button.hoverBackground": "#b19db4",
    "button.secondaryBackground": "#707070",
    "button.secondaryHoverBackground": "#292929",

    // quick picker
    "pickerGroup.border": "#292929",
    "quickInput.foreground": "#e0e0e0",
    "quickInput.background": "#191919",
    "quickInputList.focusForeground": "#f5f5f5",
    "quickInputList.focusBackground": "#292929",
    "quickInputList.focusIconForeground": "#836781",

    // widgets
    "widget.shadow": "#000000",
    "editorWidget.background": "#191919",
    "editorWidget.border": "#292929",
    "editorWidget.foreground": "#707070",
    "editorHoverWidget.background": "#191919",
    "editorHoverWidget.border": "#292929",

    // main editor view
    "editor.background": "#000000",
    "editor.foreground": "#F5F5F5",
    "editor.wordHighlightBackground": "#292929",
    "editor.selectionBackground": "#292929",
    "editor.selectionHighlightBackground": "#292929",
    "editor.selectionHighlightBorder": "#707070",
    "editor.findMatchBackground": "#292929",
    "editor.findMatchBorder": "#707070",
    "editor.lineHighlightBackground": "#191919",
    "editor.lineHighlightBorder": "#191919",
    "editorLink.activeForeground": "#F5F5F5",
    "editor.foldBackground": "#191919",

    // editor peek
    "peekView.border": "#836781",
    "peekViewEditor.background": "#191919",
    "peekViewTitle.background": "#000000",
    "peekViewResult.background": "#191919",
    "peekViewEditorGutter.background": "#292929",
    "peekViewTitleDescription.foreground": "#707070",

    // editor groups
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.border": "#292929",
    "editorGroupHeader.tabsBorder": "#191919",
    "editorGroup.border": "#191919",

    // editor hints
    "editorError.foreground": "#ab6369",
    "editorWarning.foreground": "#caab6d",
    "editorInfo.foreground": "#6f98b3",

    // editor states
    // TODO: High contrast "editorUnnecessaryCode.border": "#CAAB6D",
    "editorUnnecessaryCode.opacity": "#19191975",
    "editorOverviewRuler.errorForeground": "#ab6369",
    "editorOverviewRuler.deletedForeground": "#ab6369",
    "editorOverviewRuler.infoForeground": "#6f98b3",
    "editorOverviewRuler.warningForeground": "#caab6d",
    // "editorError.border": "#AB6369",

    // editor overview
    "editorOverviewRuler.modifiedForeground": "#CAAB6D",
    "editorOverviewRuler.incomingContentForeground": "#ab6369",
    "editorOverviewRuler.addedForeground": "#81B19B",

    // editor search & peek view matches
    "peekViewResult.matchHighlightBackground": "#292929",
    "peekViewEditor.matchHighlightBackground": "#292929",
    "peekViewEditor.matchHighlightBorder": "#707070",
    "editor.findMatchHighlightBackground": "#292929",
    "editor.findMatchHighlightBorder": "#707070",

    // minimap view
    "minimap.background": "#000000",
    "minimapSlider.background": "#83678150",
    "minimapSlider.hoverBackground": "#83678160",
    "minimap.errorHighlight": "#AB6369",
    "minimapGutter.deletedBackground": "#AB6369",
    "minimap.warningHighlight": "#CAAB6D",
    "minimapSlider.activeBackground": "#836781",
    "minimap.findMatchHighlight": "#707070",
    "minimap.selectionHighlight": "#b19db4",

    // breadcrumb
    "breadcrumb.activeSelectionForeground": "#836781",
    "breadcrumb.focusForeground": "#836781",

    // debugger breakpoints
    "debugIcon.breakpointForeground": "#AB6369",
    "debugIcon.breakpointDisabledForeground": "#1D201F",

    // main tabs
    "tab.border": "#292929",
    "tab.activeBackground": "#000000",
    "tab.activeForeground": "#F5F5F5",
    "tab.inactiveForeground": "#7D7D7D",
    "tab.inactiveBackground": "#191919",
    "tab.activeModifiedBorder": "#CAAB6D",
    "tab.inactiveModifiedBorder": "#CAAB6D",
    "tab.unfocusedActiveModifiedBorder": "#CAAB6D",
    "tab.unfocusedInactiveModifiedBorder": "#CAAB6D",

    // sidebar
    "sideBar.background": "#000000",
    "sideBar.border": "#191919",
    "sideBarTitle.foreground": "#F5F5F5",
    "sideBarSectionHeader.border": "#292929",
    "sideBarSectionHeader.background": "#000000",

    // extension stuff
    "extensionIcon.starForeground": "#CAAB6D",

    // title bar (where the settings in windows are + title)
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#F5F5F5",
    "titleBar.border": "#191919",

    // action panel (eg. terminal)
    "panel.background": "#000000",
    "panel.border": "#191919",
    "panel.dropBorder": "#191919",

    // menu (dropdowns at the top)
    "menu.background": "#000000",
    "menu.border": "#191919",
    "menu.separatorBackground": "#707070",

    // status bar (the bottom bar)
    "statusBar.background": "#000000",
    "statusBar.foreground": "#f5f5f5",
    "statusBar.border": "#191919",
    "statusBar.debuggingBackground": "#836781",
    "statusBar.debuggingForeground": "#f5f5f5",
    "statusBar.noFolderBackground": "#191919",
    "statusBarItem.errorBackground": "#ab6369",

    // activity bar (where scm, search, etc. lives in)
    "activityBar.background": "#000000",
    "activityBar.foreground": "#f5f5f5",
    "activityBar.inactiveForeground": "#707070",
    "activityBar.activeBorder": "#f5f5f5",
    "activityBar.border": "#191919",
    "activityBarBadge.foreground": "#f5f5f5",
    "activityBarBadge.background": "#836781",

    // source management control
    "editorGutter.addedBackground": "#81B19B",
    "editorGutter.deletedBackground": "#AB6369",
    "editorGutter.modifiedBackground": "#CAAB6D",
    "gitDecoration.stageDeletedResourceForeground": "#AB6369",
    "gitDecoration.stageModifiedResourceForeground": "#CAAB6D",
    "gitDecoration.addedResourceForeground": "#81B19B",
    "gitDecoration.deletedResourceForeground": "#AB6369",
    "gitDecoration.conflictingResourceForeground": "#AB6369",
    "gitDecoration.modifiedResourceForeground": "#CAAB6D",
    "gitDecoration.untrackedResourceForeground": "#81B19B",
    "gitDecoration.renamedResourceForeground": "#CAAB6D",
    "gitDecoration.ignoredResourceForeground": "#777777",
    "merge.currentHeaderBackground": "#81B19B98",
    "merge.currentContentBackground": "#81B19B40",
    "merge.incomingHeaderBackground": "#CAAB6D98",
    "merge.incomingContentBackground": "#CAAB6D40",
    "merge.border": "#191919",
    "diffEditor.insertedTextBackground": "#81B19B30",
    "diffEditor.removedTextBackground": "#AB636950",

    // icons
    "problemsErrorIcon.foreground": "#AB6369",
    "problemsInfoIcon.foreground": "#6F98B3",
    "problemsWarningIcon.foreground": "#CAAB6D",

    // debugger
    "debugToolBar.background": "#191919",
    "debugIcon.continueForeground": "#81B19B",
    "debugIcon.stepIntoForeground": "#81B19B",
    "debugIcon.stepBackForeground": "#AB6369",
    "debugIcon.stepOutForeground": "#AB6369",
    "debugIcon.stepOverForeground": "#CAAB6D",
    "debugIcon.pauseForeground": "#6F98B3",
    "debugIcon.disconnectForeground": "#AB6369",
    "debugIcon.restartForeground": "#6F98B3",
    "debugIcon.stopForeground": "#AB6369",
    "debugIcon.startForeground": "#81B19B",

    // debug console
    "debugConsole.errorForeground": "#AB6369",
    "debugConsole.infoForeground": "#6F98B3",
    "debugConsole.warningForeground": "#CAAB6D",
    "debugConsole.sourceForeground": "#f5f5f5",

    // notifications
    "notificationCenterHeader.background": "#292929",
    "notifications.background": "#191919",
    "notifications.foreground": "#FAFAFA",
    "notifications.border": "#836781",
    "notificationCenter.border": "#836781",
    "notificationLink.foreground": "#b19db4",
    "notificationsErrorIcon.foreground": "#AB6369",
    "notificationsInfoIcon.foreground": "#6f98b3",
    "notificationsWarningIcon.foreground": "#CAAB6D",
    "notificationToast.border": "#836781",

    // terminal
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#6f98b3",
    "terminal.ansiBrightBlack": "#777777",
    "terminal.ansiBrightBlue": "#6f98b3",
    "terminal.ansiCyan": "#6f98b3",
    "terminal.ansiBrightCyan": "#6f98b3",
    "terminal.ansiGreen": "#81B19B",
    "terminal.ansiBrightGreen": "#81B19B",
    "terminal.ansiMagenta": "#b19db4",
    "terminal.ansiBrightMagenta": "#b19db4",
    "terminal.ansiWhite": "#EBEBEB",
    "terminal.ansiBrightWhite": "#FAFAFA",
    "terminal.ansiYellow": "#CAAB6D",
    "terminal.ansiBrightYellow": "#CAAB6D",
    "terminal.ansiRed": "#AB6369",
    "terminal.ansiBrightRed": "#AB6369",

    // welcome page
    "welcomePage.progress.background": "#292929",
    "welcomePage.tileBackground": "#191919",
    "welcomePage.progress.foreground": "#81b19b"
  },
  "tokenColors": [
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "comment punctuation", "comment.line.number-sign"],
      "settings": {
        "foreground": "#707070c0"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ab6369"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#836781"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#836781"
      }
    },
    {
      "name": "Punctuation - String start and end",
      "scope": [
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#b19db4"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#6F98B3"
      }
    },
    {
      "name": "Block Level Properties",
      "scope": ["meta.block variable.other.property"],
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#6F98B3"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#6f98b3"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#CAAB6D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#81B19B"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#FAFAFA"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "#b89784"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b89784"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6f98b3"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "#6f98b3"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },

    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },

    // JSON
    {
      "name": "JSON - Keys",
      "scope": [
        "source.json meta.structure.dictionary support.type.property-name",
        "source.json punctuation.support.type.property-name.begin",
        "source.json punctuation.support.type.property-name.end"
      ],
      "settings": {
        "foreground": "#B19DB4"
      }
    },
    {
      "name": "JSON - Values",
      "scope": [
        "source.json string",
        "source.json meta.structure.dictionary punctuation",
        "source.json meta.structure.dictionary punctuation.definition.string.begin",
        "source.json meta.structure.dictionary punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },

    // YAML
    {
      "name": "YAML - Tag",
      "scope": ["source.yaml entity.name.tag"],
      "settings": {
        "foreground": "#b19db4"
      }
    },
    {
      "name": "YAML - Outs",
      "scope": ["source.yaml string.unquoted.plain.out"],
      "settings": {
        "foreground": "#FAFAFA"
      }
    },
    {
      "name": "YAML - Control Flow Keywords",
      "scope": ["source.yaml keyword.control.flow"],
      "settings": {
        "foreground": "#81b19b"
      }
    },
    {
      "name": "YAML - Comments",
      "scope": ["source.yaml comment"],
      "settings": {
        "foreground": "#ff0000"
      }
    },

    // Markdown
    {
      "name": "Markdown - Plain",
      "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#b19db4"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#caab6d"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#836781"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#836781"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#836781"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#b89784"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#6f98b3"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#b19db4"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#caab6d"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#b19db4"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#707070"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#ebebeb"
