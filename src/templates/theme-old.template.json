{
  "name": "<%~ it.name %>",
  "$schema": "vscode://schemas/color-theme",
  "type": "<%~ it.type %>",
  "colors": {
    "foreground": "<%~ it.grayscale.foreground %>",
    "focusBorder": "<%~ it.primary.magentaDark %>",
    "icon.foreground": "<%~ it.grayscale.foreground %>",

    // windows
    "window.activeBorder": "<%~ it.grayscale.backgroundLayer1 %>",
    "window.inactiveBorder": "<%~ it.grayscale.layerBorder %>",

    // links
    "textLink.foreground": "<%~ it.primary.magentaDark %>",
    "textLink.activeForeground": "<%~ it.primary.magentaLight %>",

    // checkboxes
    "checkbox.background": "<%~ it.primary.magentaDark %>",
    "settings.checkboxBackground": "<%~ it.grayscale.layerBorder %>",
    "settings.checkboxForeground": "<%~ it.primary.magentaDark %>",

    // dropdowns
    "dropdown.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "dropdown.border": "<%~ it.grayscale.layerBorder %>",
    "settings.dropdownBorder": "<%~ it.grayscale.layerBorder %>",

    // inputs
    "input.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "input.border": "<%~ it.grayscale.layerBorder %>",

    // progress bars
    "progressBar.background": "<%~ it.primary.magentaDark %>",

    // scrollbars
    "scrollbarSlider.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "scrollbarSlider.activeBackground": "<%~ it.grayscale.layerBorder %>",
    "scrollbarSlider.hoverBackground": "<%~ it.grayscale.layerBorder %>",

    // generic lists
    "list.errorForeground": "<%~ it.primary.red %>",
    "list.invalidItemForeground": "<%~ it.primary.yellow %>",
    "list.warningForeground": "<%~ it.primary.yellow %>",
    "list.hoverBackground": "<%~ it.grayscale.backgroundLayer2 %>",
    "list.activeSelectionBackground": "<%~ it.grayscale.backgroundLayer2 %>",
    "list.highlightForeground": "<%~ it.primary.magentaDark %>",

    // buttons
    "button.background": "<%~ it.primary.magentaDark %>",
    "button.hoverBackground": "<%~ it.primary.magentaLight %>",
    "button.secondaryBackground": "<%~ it.grayscale.backgroundLayerFocus %>",
    "button.secondaryHoverBackground": "<%~ it.grayscale.layerBorder %>",

    // quick picker
    "pickerGroup.border": "<%~ it.grayscale.layerBorder %>",
    "quickInput.foreground": "#e0e0e0",
    "quickInput.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "quickInputList.focusForeground": "<%~ it.grayscale.foreground %>",
    "quickInputList.focusBackground": "<%~ it.grayscale.layerBorder %>",
    "quickInputList.focusIconForeground": "<%~ it.primary.magentaDark %>",

    // widgets
    "widget.shadow": "<%~ it.grayscale.backgroundLayer1 %>",
    "editorWidget.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "editorWidget.border": "<%~ it.grayscale.layerBorder %>",
    "editorWidget.foreground": "<%~ it.grayscale.backgroundLayerFocus %>",
    "editorHoverWidget.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "editorHoverWidget.border": "<%~ it.grayscale.layerBorder %>",

    // main editor view
    "editor.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "editor.foreground": "<%~ it.grayscale.foreground %>",
    "editor.wordHighlightBackground": "<%~ it.grayscale.layerBorder %>",
    "editor.selectionBackground": "<%~ it.grayscale.layerBorder %>",
    "editor.selectionHighlightBackground": "<%~ it.grayscale.layerBorder %>",
    "editor.selectionHighlightBorder": "<%~ it.grayscale.backgroundLayerFocus %>",
    "editor.findMatchBackground": "<%~ it.grayscale.layerBorder %>",
    "editor.findMatchBorder": "<%~ it.grayscale.backgroundLayerFocus %>",
    "editor.lineHighlightBackground": "<%~ it.grayscale.backgroundLayer2 %>",
    "editor.lineHighlightBorder": "<%~ it.grayscale.backgroundLayer2 %>",
    "editorLink.activeForeground": "<%~ it.grayscale.foreground %>",
    "editor.foldBackground": "<%~ it.grayscale.backgroundLayer2 %>",

    // editor peek
    "peekView.border": "<%~ it.primary.magentaDark %>",
    "peekViewEditor.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "peekViewTitle.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "peekViewResult.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "peekViewEditorGutter.background": "<%~ it.grayscale.layerBorder %>",
    "peekViewTitleDescription.foreground": "<%~ it.grayscale.backgroundLayerFocus %>",

    // editor groups
    "editorGroupHeader.tabsBackground": "<%~ it.grayscale.backgroundLayer1 %>",
    "editorGroupHeader.tabsBorder": "<%~ it.grayscale.backgroundLayer2 %>",
    "editorGroupHeader.border": "<%~ it.grayscale.layerBorder %>",
    "editorGroup.border": "<%~ it.grayscale.backgroundLayer2 %>",

    // editor hints
    "editorError.foreground": "<%~ it.primary.red %>",
    "editorWarning.foreground": "<%~ it.primary.yellow %>",
    "editorInfo.foreground": "<%~ it.primary.blue %>",

    // editor states
    // TODO: High contrast "editorUnnecessaryCode.border": "<%~ it.primary.yellow %>",
    "editorUnnecessaryCode.opacity": "<%~ it.grayscale.backgroundLayer2 %>75",
    "editorOverviewRuler.errorForeground": "<%~ it.primary.red %>",
    "editorOverviewRuler.deletedForeground": "<%~ it.primary.red %>",
    "editorOverviewRuler.infoForeground": "<%~ it.primary.blue %>",
    "editorOverviewRuler.warningForeground": "<%~ it.primary.yellow %>",
    // "editorError.border": "<%~ it.primary.red %>",

    // editor overview
    "editorOverviewRuler.modifiedForeground": "<%~ it.primary.yellow %>",
    "editorOverviewRuler.incomingContentForeground": "<%~ it.primary.red %>",
    "editorOverviewRuler.addedForeground": "<%~ it.primary.green %>",

    // editor search & peek view matches
    "peekViewResult.matchHighlightBackground": "<%~ it.grayscale.layerBorder %>",
    "peekViewEditor.matchHighlightBackground": "<%~ it.grayscale.layerBorder %>",
    "peekViewEditor.matchHighlightBorder": "<%~ it.grayscale.backgroundLayerFocus %>",
    "editor.findMatchHighlightBackground": "<%~ it.grayscale.layerBorder %>",
    "editor.findMatchHighlightBorder": "<%~ it.grayscale.backgroundLayerFocus %>",

    // minimap view
    "minimap.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "minimap.errorHighlight": "<%~ it.primary.red %>",
    "minimap.warningHighlight": "<%~ it.primary.yellow %>",
    "minimap.findMatchHighlight": "<%~ it.grayscale.backgroundLayerFocus %>",
    "minimap.selectionHighlight": "<%~ it.primary.magentaLight %>",
    "minimapGutter.deletedBackground": "<%~ it.primary.red %>",
    "minimapSlider.background": "<%~ it.primary.magentaDark %>50",
    "minimapSlider.hoverBackground": "<%~ it.primary.magentaDark %>55",
    "minimapSlider.activeBackground": "<%~ it.primary.magentaDark %>65",

    // breadcrumb
    "breadcrumb.activeSelectionForeground": "<%~ it.primary.magentaDark %>",
    "breadcrumb.focusForeground": "<%~ it.primary.magentaDark %>",

    // debugger breakpoints
    "debugIcon.breakpointForeground": "<%~ it.primary.red %>",
    "debugIcon.breakpointDisabledForeground": "#1D201F",

    // main tabs
    "tab.border": "<%~ it.grayscale.layerBorder %>",
    "tab.activeBackground": "<%~ it.grayscale.backgroundLayer2 %>",
    "tab.activeForeground": "<%~ it.grayscale.foreground %>",
    "tab.inactiveForeground": "<%~ it.grayscale.foreground %>50",
    "tab.inactiveBackground": "<%~ it.grayscale.backgroundLayer1 %>",
    "tab.activeModifiedBorder": "<%~ it.primary.yellow %>",
    "tab.inactiveModifiedBorder": "<%~ it.primary.yellow %>",
    "tab.unfocusedActiveModifiedBorder": "<%~ it.primary.yellow %>",
    "tab.unfocusedInactiveModifiedBorder": "<%~ it.primary.yellow %>",

    // sidebar
    "sideBar.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "sideBar.border": "<%~ it.grayscale.backgroundLayer2 %>",
    "sideBarTitle.foreground": "<%~ it.grayscale.foreground %>",
    "sideBarSectionHeader.border": "<%~ it.grayscale.layerBorder %>",
    "sideBarSectionHeader.background": "<%~ it.grayscale.backgroundLayer1 %>",

    // extension stuff
    "extensionIcon.starForeground": "<%~ it.primary.yellow %>",

    // title bar (where the settings in windows are + title)
    "titleBar.activeBackground": "<%~ it.grayscale.backgroundLayer1 %>",
    "titleBar.activeForeground": "<%~ it.grayscale.foreground %>",
    "titleBar.border": "<%~ it.grayscale.backgroundLayer2 %>",

    // action panel (eg. terminal)
    "panel.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "panel.border": "<%~ it.grayscale.backgroundLayer2 %>",
    "panel.dropBorder": "<%~ it.grayscale.backgroundLayer2 %>",

    // menu (dropdowns at the top)
    "menu.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "menu.border": "<%~ it.grayscale.backgroundLayer2 %>",
    "menu.separatorBackground": "<%~ it.grayscale.backgroundLayerFocus %>",

    // status bar (the bottom bar)
    "statusBar.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "statusBar.foreground": "<%~ it.grayscale.foreground %>",
    "statusBar.border": "<%~ it.grayscale.backgroundLayer2 %>",
    "statusBar.debuggingBackground": "<%~ it.primary.magentaDark %>",
    "statusBar.debuggingForeground": "<%~ it.grayscale.foreground %>",
    "statusBar.noFolderBackground": "<%~ it.grayscale.backgroundLayer2 %>",
    "statusBarItem.errorBackground": "<%~ it.primary.red %>",

    // activity bar (where scm, search, etc. lives in)
    "activityBar.background": "<%~ it.grayscale.backgroundLayer1 %>",
    "activityBar.foreground": "<%~ it.grayscale.foreground %>",
    "activityBar.inactiveForeground": "<%~ it.grayscale.backgroundLayerFocus %>",
    "activityBar.activeBorder": "<%~ it.grayscale.foreground %>",
    "activityBar.border": "<%~ it.grayscale.backgroundLayer2 %>",
    "activityBarBadge.foreground": "<%~ it.grayscale.foreground %>",
    "activityBarBadge.background": "<%~ it.primary.magentaDark %>",

    // source management control
    "editorGutter.addedBackground": "<%~ it.primary.green %>",
    "editorGutter.deletedBackground": "<%~ it.primary.red %>",
    "editorGutter.modifiedBackground": "<%~ it.primary.yellow %>",
    "gitDecoration.stageDeletedResourceForeground": "<%~ it.primary.red %>",
    "gitDecoration.stageModifiedResourceForeground": "<%~ it.primary.yellow %>",
    "gitDecoration.addedResourceForeground": "<%~ it.primary.green %>",
    "gitDecoration.deletedResourceForeground": "<%~ it.primary.red %>",
    "gitDecoration.conflictingResourceForeground": "<%~ it.primary.red %>",
    "gitDecoration.modifiedResourceForeground": "<%~ it.primary.yellow %>",
    "gitDecoration.untrackedResourceForeground": "<%~ it.primary.green %>",
    "gitDecoration.renamedResourceForeground": "<%~ it.primary.yellow %>",
    "gitDecoration.ignoredResourceForeground": "<%~ it.grayscale.backgroundLayerFocus %>",
    "merge.currentHeaderBackground": "<%~ it.primary.green %>98",
    "merge.currentContentBackground": "<%~ it.primary.green %>40",
    "merge.incomingHeaderBackground": "<%~ it.primary.yellow %>98",
    "merge.incomingContentBackground": "<%~ it.primary.yellow %>40",
    "merge.border": "<%~ it.grayscale.backgroundLayer2 %>",
    "diffEditor.insertedTextBackground": "<%~ it.primary.green %>30",
    "diffEditor.removedTextBackground": "<%~ it.primary.red %>30",

    // icons
    "problemsErrorIcon.foreground": "<%~ it.primary.red %>",
    "problemsInfoIcon.foreground": "<%~ it.primary.blue %>",
    "problemsWarningIcon.foreground": "<%~ it.primary.yellow %>",

    // debugger
    "debugToolBar.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "debugIcon.continueForeground": "<%~ it.primary.green %>",
    "debugIcon.stepIntoForeground": "<%~ it.primary.green %>",
    "debugIcon.stepBackForeground": "<%~ it.primary.red %>",
    "debugIcon.stepOutForeground": "<%~ it.primary.red %>",
    "debugIcon.stepOverForeground": "<%~ it.primary.yellow %>",
    "debugIcon.pauseForeground": "<%~ it.primary.blue %>",
    "debugIcon.disconnectForeground": "<%~ it.primary.red %>",
    "debugIcon.restartForeground": "<%~ it.primary.blue %>",
    "debugIcon.stopForeground": "<%~ it.primary.red %>",
    "debugIcon.startForeground": "<%~ it.primary.green %>",

    // debug console
    "debugConsole.errorForeground": "<%~ it.primary.red %>",
    "debugConsole.infoForeground": "<%~ it.primary.blue %>",
    "debugConsole.warningForeground": "<%~ it.primary.yellow %>",
    "debugConsole.sourceForeground": "<%~ it.grayscale.foreground %>",

    // notifications
    "notificationCenterHeader.background": "<%~ it.grayscale.layerBorder %>",
    "notifications.background": "<%~ it.grayscale.backgroundLayer2 %>",
    "notifications.foreground": "<%~ it.grayscale.foreground %>",
    "notifications.border": "<%~ it.primary.magentaDark %>",
    "notificationCenter.border": "<%~ it.primary.magentaDark %>",
    "notificationLink.foreground": "<%~ it.primary.magentaLight %>",
    "notificationsErrorIcon.foreground": "<%~ it.primary.red %>",
    "notificationsInfoIcon.foreground": "<%~ it.primary.blue %>",
    "notificationsWarningIcon.foreground": "<%~ it.primary.yellow %>",
    "notificationToast.border": "<%~ it.primary.magentaDark %>",

    // terminal
    "terminal.ansiBlack": "<%~ it.grayscale.backgroundLayer1 %>",
    "terminal.ansiBlue": "<%~ it.primary.blue %>",
    "terminal.ansiBrightBlack": "#777777",
    "terminal.ansiBrightBlue": "<%~ it.primary.blue %>",
    "terminal.ansiCyan": "<%~ it.primary.blue %>",
    "terminal.ansiBrightCyan": "<%~ it.primary.blue %>",
    "terminal.ansiGreen": "<%~ it.primary.green %>",
    "terminal.ansiBrightGreen": "<%~ it.primary.green %>",
    "terminal.ansiMagenta": "<%~ it.primary.magentaLight %>",
    "terminal.ansiBrightMagenta": "<%~ it.primary.magentaLight %>",
    "terminal.ansiWhite": "<%~ it.grayscale.foreground %>",
    "terminal.ansiBrightWhite": "<%~ it.grayscale.foreground %>",
    "terminal.ansiYellow": "<%~ it.primary.yellow %>",
    "terminal.ansiBrightYellow": "<%~ it.primary.yellow %>",
    "terminal.ansiRed": "<%~ it.primary.red %>",
    "terminal.ansiBrightRed": "<%~ it.primary.red %>",

    // welcome page
    "welcomePage.progress.background": "<%~ it.grayscale.layerBorder %>",
    "welcomePage.tileBackground": "<%~ it.grayscale.backgroundLayer2 %>",
    "welcomePage.progress.foreground": "<%~ it.primary.green %>"
  },
  "tokenColors": [
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "<%~ it.grayscale.foreground %>"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "<%~ it.primary.brown %>"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment punctuation",
        "comment.line.number-sign",
        "source.json punctuation.definition.comment.json.comments"
      ],
      "settings": {
        "foreground": "<%~ it.grayscale.backgroundLayerFocus %>c0"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "<%~ it.grayscale.foreground %>"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "<%~ it.primary.red %>"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "<%~ it.primary.magentaDark %>"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "<%~ it.primary.magentaLight %>"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "<%~ it.primary.magentaDark %>"
      }
    },
    {
      "name": "Punctuation - String start and end",
      "scope": [
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "<%~ it.primary.yellow %>"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "<%~ it.primary.yellow %>"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "<%~ it.primary.magentaLight %>"
      }
    },
    // {
    //   "name": "Block Level Variables",
    //   "scope": ["meta.block variable.other"],
    //   "settings": {
    //     "foreground": "<%~ it.primary.blue %>"
    //   }
    // },
    // {
    //   "name": "Block Level Properties",
    //   "scope": ["meta.block variable.other.property"],
    //   "settings": {
    //     "foreground": "<%~ it.grayscale.foreground %>"
    //   }
    // },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "<%~ it.primary.blue %>"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        // "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "<%~ it.primary.blue %>"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "<%~ it.primary.yellow %>"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "<%~ it.primary.green %>"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "<%~ it.primary.green %>"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "<%~ it.primary.brown %>"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "<%~ it.primary.brown %>"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "<%~ it.primary.blue %>"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "<%~ it.primary.blue %>"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "<%~ it.primary.magentaLight %>"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "<%~ it.primary.yellow %>"
      }
    },

    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "<%~ it.primary.green %>"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },

    // JSON
    {
      "name": "JSON - Keys",
      "scope": [
        "source.json meta.structure.dictionary support.type.property-name",
        "source.json punctuation.support.type.property-name.begin",
        "source.json punctuation.support.type.property-name.end"
      ],
      "settings": {
        "foreground": "<%~ it.primary.magentaLight %>"
      }
    },
    {
      "name": "JSON - Values",
      "scope": [
        "source.json string",
        "source.json meta.structure.dictionary punctuation",
        "source.json meta.structure.dictionary punctuation.definition.string.begin",
        "source.json meta.structure.dictionary punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "<%~ it.grayscale.foreground %>"
      }
    },

    // YAML
    {
      "name": "YAML - Tag",
      "scope": ["source.yaml entity.name.tag"],
      "settings": {
        "foreground": "<%~ it.primary.magentaLight %>"
      }
    },
    {
      "name": "YAML - Outs",
      "scope": ["source.yaml string.unquoted.plain.out"],
      "settings": {
        "foreground": "#FAFAFA"
      }
    },
    {
      "name": "YAML - Control Flow Keywords",
      "scope": ["source.yaml keyword.control.flow"],
      "settings": {
        "foreground": "<%~ it.primary.green %>"
      }
    },

    // Markdown
    {
      "name": "Markdown - Plain",
      "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "<%~ it.grayscale.foreground %>"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "<%~ it.primary.magentaLight %>"
      }
    },
    {
