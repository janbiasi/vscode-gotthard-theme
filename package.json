{
  "private": true,
  "name": "gotthard-theme",
  "displayName": "Gotthard Theme",
  "description": "A nature inspired matte theme for everyday use.",
  "version": "1.3.3",
  "icon": "assets/icon.png",
  "publisher": "JanBiasi",
  "author": {
    "name": "Jan Biasi",
    "url": "https://janbiasi.ch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janbiasi/vscode-gotthard-theme.git"
  },
  "license": "MIT",
  "scripts": {
    "package": "vsce package -o ./bin/gotthard.vsix",
    "publish": "vsce publish",
    "prettier": "prettier -w \"./**/*.{json,js,md,yml}\"",
    "release": "semantic-release",
    "generate": "ts-node ./src/generator.ts"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": ["Themes"],
  "contributes": {
    "themes": [
      {
        "label": "Gotthard",
        "uiTheme": "vs-dark",
        "path": "./dist/gotthard.json"
      },
      {
        "label": "Gotthard - Medium Dark",
        "uiTheme": "vs-dark",
        "path": "./dist/gotthard-medium-dark.json"
      },
      {
        "label": "Gotthard - Light",
        "uiTheme": "vs",
        "path": "./dist/gotthard-light.json"
      }
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/node": "16.7.1",
    "eta": "1.12.3",
    "fs-extra": "10.0.0",
    "husky": "7.0.1",
    "prettier": "2.3.2",
    "semantic-release": "17.4.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "vsce": "1.96.1"
  }
}
