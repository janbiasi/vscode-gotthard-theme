{
  "private": true,
  "name": "gotthard-theme",
  "displayName": "Gotthard Theme",
  "description": "A nature inspired matte theme for everyday use.",
  "version": "1.3.1",
  "icon": "assets/icon.png",
  "publisher": "JanBiasi",
  "badges": [
    {
      "url": "https://img.shields.io/vscode-marketplace/v/janbiasi.gotthard-theme.svg",
      "href": "https://github.com/janbiasi/vscode-gotthard-theme/releases",
      "description": "Version"
    }
  ],
  "author": {
    "name": "Jan Biasi",
    "url": "https://janbiasi.ch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janbiasi/vscode-gotthard-theme.git"
  },
  "license": "MIT",
  "scripts": {
    "package": "vsce package -o ./bin/gotthard.vsix",
    "publish": "vsce publish",
    "prettier": "prettier -w \"./**/*.{json,js,md,yml}\"",
    "release": "semantic-release"
  },
  "engines": {
